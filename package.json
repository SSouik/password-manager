{
    "name": "password-manager",
    "version": "0.0.1",
    "license": "MIT",
    "scripts": {
        "ng": "nx",
        "build": "nx run-many --target=build --all --parallel=4",
        "build:apps:dev": "nx run-many --target=build --projects=tag:app --configuration=development",
        "build:apps:prod": "nx run-many --target=build --projects=tag:app --configuration=production",
        "build:libs:dev": "nx run-many --target=build --projects=tag:lib --configuration=development",
        "build:libs:prod": "nx run-many --target=build --projects=tag:lib --configuration=production",
        "build:config": "nx build --project=password-manager-config",
        "build:crypto": "nx build --project=password-manager-crypto",
        "build:dynamodb-client": "nx build --project=password-manager-dynamodb-client",
        "build:logger": "nx build --project=password-manager-logger",
        "build:types": "nx build --project=password-manager-types",
        "build:dev": "yarn build --configuration=development",
        "build:ui:dev": "nx build --project=password-manager-ui --configuration=development",
        "build:api:dev": "nx build --project=password-manager-api --configuration=development",
        "build:prod": "yarn build --configuration=production",
        "build:ui:prod": "nx build --project=password-manager-ui --configuration=production",
        "build:api:prod": "nx build --project=password-manager-api --configuration=production",
        "start": "yarn start:api",
        "start:api": "nx serve --project=password-manager-api --configuration=development",
        "start:ui": "nx serve --project=password-manager-ui --configuration=development --port=4200",
        "test": "nx run-many --target=test --all --parallel=4",
        "test:coverage": "yarn test --codeCoverage",
        "test:ui": "nx test --project=password-manager-ui",
        "test:ui:coverage": "yarn test:ui --codeCoverage",
        "test:api": "nx test --project=password-manager-api",
        "test:api:coverage": "yarn test:api --codeCoverage",
        "test:config": "nx test --project=password-manager-config",
        "test:config:coverage": "yarn test:config --codeCoverage",
        "test:crypto": "nx test --project=password-manager-crypto",
        "test:crypto:coverage": "yarn test:crypto --codeCoverage",
        "test:dynamodb-client": "nx test --project=password-manager-dynamodb-client",
        "test:dynamodb-client:coverage": "yarn test:dynamodb-client --codeCoverage",
        "test:logger": "nx test --project=password-manager-logger",
        "test:logger:coverage": "yarn test:logger --codeCoverage",
        "lint": "nx run-many --target=lint --all --parallel=4",
        "lint:fix": "yarn lint --fix",
        "lint:ui": "nx lint --project=password-manager-ui",
        "lint:ui:fix": "yarn lint:ui --fix",
        "lint:api": "nx lint --project=password-manager-api",
        "lint:api:fix": "yarn lint:api --fix",
        "lint:e2e": "nx lint --project=password-manager-ui-e2e",
        "lint:e2e:fix": "yarn lint:e2e --fix",
        "lint:config": "nx lint --project=password-manager-config",
        "lint:config:fix": "yarn lint:config --fix",
        "lint:crypto": "nx lint --project=password-manager-crypto",
        "lint:crypto:fix": "yarn lint:crypto --fix",
        "lint:dynamodb-client": "nx lint --project=password-manager-dynamodb-client",
        "lint:dynamodb-client:fix": "yarn lint:dynamodb-client --fix",
        "lint:logger": "nx lint --project=password-manager-logger",
        "lint:logger:fix": "yarn lint:logger --fix",
        "lint:types": "nx lint --project=password-manager-types",
        "lint:types:fix": "yarn lint:types --fix",
        "lint:scss": "stylelint **/*.scss",
        "lint:scss:fix": "yarn lint:scss --fix",
        "format:check": "nx format:check --all",
        "format:write": "nx format:write --all",
        "dynamo:admin": "set DYNAMO_ENDPOINT=http://localhost:7777 && dynamodb-admin -p 8080",
        "dynamo:tables:create": "ts-node ./data/create-tables.ts",
        "dynamo:tables:delete": "ts-node ./data/delete-tables.ts",
        "dynamo:data:create": "ts-node ./data/create-sample-data.ts",
        "docker:build": "docker-compose build",
        "docker:up": "docker-compose up",
        "docker:down": "docker-compose down"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~15.1.0",
        "@angular/cdk": "^15.1.4",
        "@angular/common": "~15.1.0",
        "@angular/compiler": "~15.1.0",
        "@angular/core": "~15.1.0",
        "@angular/forms": "~15.1.0",
        "@angular/material": "^15.1.4",
        "@angular/platform-browser": "~15.1.0",
        "@angular/platform-browser-dynamic": "~15.1.0",
        "@angular/router": "~15.1.0",
        "@aws-sdk/client-dynamodb": "^3.252.0",
        "@aws-sdk/lib-dynamodb": "^3.252.0",
        "@nestjs/common": "^9.0.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/serve-static": "^3.0.0",
        "chalk": "4.1.2",
        "crypto": "^1.0.1",
        "jose": "^4.12.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "uuid": "^9.0.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~15.1.0",
        "@angular-eslint/eslint-plugin": "~15.0.0",
        "@angular-eslint/eslint-plugin-template": "~15.0.0",
        "@angular-eslint/template-parser": "~15.0.0",
        "@angular/cli": "~15.1.0",
        "@angular/compiler-cli": "~15.1.0",
        "@angular/language-service": "~15.1.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@nrwl/angular": "^15.5.2",
        "@nrwl/cypress": "15.5.2",
        "@nrwl/eslint-plugin-nx": "15.5.2",
        "@nrwl/jest": "15.5.2",
        "@nrwl/js": "^15.5.2",
        "@nrwl/linter": "15.5.2",
        "@nrwl/nest": "15.5.2",
        "@nrwl/node": "15.5.2",
        "@nrwl/nx-cloud": "latest",
        "@nrwl/webpack": "15.5.2",
        "@nrwl/workspace": "15.5.2",
        "@types/jest": "28.1.1",
        "@types/node": "16.11.7",
        "@types/uuid": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "cypress": "^12.2.0",
        "dotenv": "^16.0.3",
        "dynamodb-admin": "^4.5.0",
        "eslint": "~8.15.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-cypress": "^2.10.3",
        "eslint-plugin-import": "^2.27.5",
        "jest": "28.1.1",
        "jest-environment-jsdom": "28.1.1",
        "jest-preset-angular": "~12.2.3",
        "nodemon": "^2.0.20",
        "nx": "15.5.2",
        "prettier": "^2.6.2",
        "stylelint": "^15.1.0",
        "stylelint-config-standard": "^30.0.1",
        "stylelint-config-standard-scss": "^7.0.1",
        "stylelint-scss": "^4.4.0",
        "ts-jest": "28.0.5",
        "ts-node": "^10.9.1",
        "typescript": "~4.8.2"
    }
}
