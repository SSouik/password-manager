{
    "bracketSameLine": false,
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "css",
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4,
    "overrides": [
        {
            "files": ["*.html"],
            "options": {
                "useTabs": false,
                "tabWidth": 2
            }
        }
    ]
}
